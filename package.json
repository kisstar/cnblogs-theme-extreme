{
  "name": "@kisstar/cnblogs-theme-extreme",
  "version": "0.0.2",
  "private": false,
  "description": "Black and white, a brief and extreme cnblogs theme.",
  "scripts": {
    "start": "node scripts/start",
    "build": "node scripts/build",
    "lint:html": "htmlhint public/**/*.{htm,html}",
    "lint:style": "stylelint --allow-empty-input \"src/**/*.{css,scss,sass}\"",
    "lint:es": "eslint --no-error-on-unmatched-pattern --cache --ext .ts,.js .",
    "lint:md": "markdownlint **/*.md",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint": "concurrently npm:lint:*",
    "commit": "git-cz",
    "prettier": "prettier -c --write \"**/*\"",
    "release": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "keywords": [
    "cnblogs",
    "cnblogs-theme-extreme"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kisstar/cnblogs-theme-extreme.git"
  },
  "author": "kisstar <dwh.chn@foxmail.com>",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.1",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "detect-port": "^1.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-unicorn": "^22.0.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^4.5.0",
    "htmlhint": "^0.14.1",
    "husky": "^4.3.0",
    "inquirer": "^7.3.3",
    "lint-staged": "^10.4.0",
    "markdownlint": "0.21.0",
    "markdownlint-cli": "^0.23.2",
    "mini-css-extract-plugin": "^0.11.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "pnp-webpack-plugin": "^1.6.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^4.0.2",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^5.0.2",
    "prettier": "2.1.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "standard-version": "9.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
